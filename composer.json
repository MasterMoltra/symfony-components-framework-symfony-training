{
    "name": "mastermoltra/microsym-framework",
    "description": "A minimal own framework based on Symfony components",
    "type": "project",
    "require": {
        "symfony/http-foundation": "^5.1",
        "symfony/routing": "^5.1",
        "symfony/http-kernel": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2"
    },
    "autoload": {
        "psr-4": {
            "App\\Controller\\": "src/Controller"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "license": "MIT"
}
